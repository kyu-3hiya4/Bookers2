<%= render 'books/list', user: @user, book: @book %>

<h2>Users</h2>

<table>
 <thead>
  <tr>
  <th>image</th>
  <th>name</th>
  <th colspan="3"></th>
  </tr>
 </thead>

 <tbody>

  <% @users.each do |user| %>
   <tr>
     <td>
     <%= image_tag user.get_profile_image(100,100) %>
     </td>
     <td>
     <%= user.neme %>
     </td>
     <td><%= link_to user_path(book.id) %> </td>
     </tr>
   <% end %>
  </tbody>
</table>